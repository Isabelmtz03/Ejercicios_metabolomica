################ 
 
Nombre <- c("Renata", "Diego", "Isa", "Lidia") 
Altura <- c(1.63, 1.80, 1.67, 1.48) 
Tabla1 <- data.frame(Nombre=Nombre, Altura=Altura) 
View(Tabla1) #Para visualizar la tabla 
head(Tabla1) 
#################### 
 
Grupo <- c("Perro", "Gato", "Cho", "Vero") 
Valor <- c(12, 25, 530000, 1350000) 
Tabla2 <- data.frame(Grupo=Grupo, Valor=Valor) 
View(Tabla2) #Para visualizar la tabla 
head(Tabla2) 
